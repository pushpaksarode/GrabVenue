                            android:text="SAT, 17 Sep 2022" />

                    </LinearLayout>


                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_marginTop="10dp"
                    android:background="@color/ultralightblue"
                    android:orientation="horizontal"
                    android:padding="10sp">

                    <ImageView
                        android:id="@+id/imageView50"
                        android:layout_width="121dp"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/hockeylogo" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView30"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:textColor="@color/black"
                            android:text="Hockey Ground"
                            android:textSize="20dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/textVie2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:textStyle="bold"
                            android:text="Near Block 10, Eshwar Nagar" />

                        <TextView
                            android:id="@+id/textView9"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="SAT, 17 Sep 2022" />

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

        </ScrollView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:background="#F3EBEB"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingTop="10sp"
                android:paddingBottom="5sp">

                <ImageButton
                    android:id="@+id/homeBtn"
                    android:layout_width="40dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="40sp"
                    android:layout_marginRight="40sp"
                    android:background="@android:color/transparent"
                    android:onClick="btn"
                    android:scaleType="fitCenter"
                    android:src="@drawable/ic_baseline_home_24" />

                <ImageButton
                    android:id="@+id/historyBtn"
                    android:layout_width="40dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="40sp"
                    android:layout_marginRight="40sp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitCenter"
                    android:src="@drawable/historybutton" />

                <ImageButton
                    android:id="@+id/settingsbtn"
                    android:layout_width="40dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="40sp"
                    android:layout_marginRight="40sp"
                    android:background="@android:color/transparent"
                    android:scaleType="fitCenter"
                    android:src="@drawable/settingsbutton" />
            </LinearLayout>
